@prefix : <http://www.semanticweb.org/bookontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/bookontology> .

<http://www.semanticweb.org/bookontology> rdf:type owl:Ontology ;
                                           <http://purl.org/dc/elements/1.1/creator> "Created by Paco" ;
                                           <http://purl.org/dc/elements/1.1/rights> "CC-BYSA" ;
                                           rdfs:label "csddsa"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/rights
<http://purl.org/dc/elements/1.1/rights> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator#Creator
<http://purl.org/dc/terms/creator#Creator> rdf:type owl:AnnotationProperty ;
                                           rdfs:subPropertyOf rdfs:comment .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/bookontology#isWrittenBy
:isWrittenBy rdf:type owl:ObjectProperty ;
             owl:inverseOf :writes ;
             rdfs:label "Is written by"@en .


###  http://www.semanticweb.org/bookontology#writes
:writes rdf:type owl:ObjectProperty ;
        rdfs:domain :Writer ;
        rdfs:range :Book ;
        rdfs:label "writes"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/bookontology#writerName
:writerName rdf:type owl:DatatypeProperty ;
            rdfs:domain :Writer ;
            rdfs:range xsd:string ;
            rdfs:comment "This property represents the name of a writer"@en ;
            rdfs:label "Writer Name"@en .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/bookontology#Book
:Book rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :isWrittenBy ;
                        owl:someValuesFrom :Writer
                      ] ;
      rdfs:comment "This is a book"@en ;
      rdfs:label "Book"@en ,
                 "Libro"@es .


###  http://www.semanticweb.org/bookontology#Comic
:Comic rdf:type owl:Class ;
       rdfs:subClassOf :Book .


###  http://www.semanticweb.org/bookontology#Poem
:Poem rdf:type owl:Class ;
      rdfs:comment "This class represents a poem"@en ;
      rdfs:label "Poem"@en .


###  http://www.semanticweb.org/bookontology#Writer
:Writer rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :writes ;
                              owl:someValuesFrom :Book
                            ] .


#################################################################
#    Individuals
#################################################################

###  http://www.semanticweb.org/bookontology#Book1
:Book1 rdf:type owl:NamedIndividual ,
                :Book .


###  http://www.semanticweb.org/bookontology#Poem1
:Poem1 rdf:type owl:NamedIndividual ,
                :Poem .


###  http://www.semanticweb.org/bookontology#Writer1
:Writer1 rdf:type owl:NamedIndividual ,
                  :Writer ;
         :writes :Poem1 .


